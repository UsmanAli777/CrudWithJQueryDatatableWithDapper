@model IEnumerable<CrudWithJQueryDatatable.Models.UserRoleEdit>

@{
    Layout = null;
}

<form action="@Url.Content("~/Role/EditUser")" id="myform" method="post">
    <div class="form-horizontal">
        <h4>Edit User Role</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <h4>@ViewBag.Email </h4>
    </div>
    <div class="form-group">
        <div class="col-md-6">

            @foreach (var item in Model)
            {
                Html.RenderPartial("_UserListPartial", item);<br />
            }
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <!--<input type="submit" class="btn btn-primary" value="Create"--> @*class="btn btn-default"*@ <!-- />-->
                <input type="submit" value="Create" class="btn btn-success" />
                <button type="submit" class="btn btn-primary" data-dismiss="modal">Cancel</button>
                @* <button type="submit" class="btn btn-success" data-dismiss="modal">Create</button>*@
            </div>
        </div>
    </div>
</form>